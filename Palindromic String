#include<bits/stdc++.h>
using namespace std;

int main()
{
    string a;
    cin>>a;
    int i,l,c=0;
    l=a.length();
    for(i=0;i<l/2;i++)
    {
        if(a[i]!=a[l-i-1])
        {
            c=1;
            break;
        }
    }
    if(c==1)
        cout<<"NO";
    else
        cout<<"YES";
}
